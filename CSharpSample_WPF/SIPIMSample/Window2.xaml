<Window x:Class="SIPIMSample.Window2"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    Title="Online status requrest" Height="215" Width="408" Loaded="Window_Loaded">
    <Canvas>
        <GroupBox Canvas.Left="12" Canvas.Top="12" Height="152" Name="groupBox1" Width="359">
            <Canvas>
                <Label Canvas.Left="6" Canvas.Top="6" Height="28" Name="labelContactName" Width="328"></Label>
                <Label Canvas.Left="6" Canvas.Top="40" Height="28" Name="label2" Width="328">wants to communicate with you and see your status</Label>
                <Button Canvas.Left="24" Canvas.Top="85" Height="23" Name="button1" Width="75" Click="button1_Click">Accept</Button>
                <Button Canvas.Left="225" Canvas.Top="85" Height="23" Name="button2" Width="75" Click="button2_Click">Reject</Button>
            </Canvas>
        </GroupBox>
    </Canvas>
</Window>
